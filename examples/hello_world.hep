Kernel32 :: #foreign_system_library "kernel32";
WriteFile :: (handle: *void, to_write: *void, length: *s32) -> bool #foreign Kernel32;
GetStdHandle :: (handle_type: s32) -> *void #foreign Kernel32;

print :: (to_print: *void) {
    handle := GetStdHandle(-11);
    WriteFile(handle, to_print, *to_print.len);
}

hello_world :: () {
    print("hello world\n");
}

#run hello_world()